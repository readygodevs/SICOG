@model IEnumerable<TesoreriaVS12.Areas.Tesoreria.Models.CA_InHabilModel>
@{
    ViewBag.Title = "Catálogo de Estados";
    Layout = null;
    var imagePath = Server.MapPath("~/Images");
    var cssPath = Server.MapPath("~/Content");
}
@Html.Partial("PdfHeader")
<link href="@cssPath/bootstrap.min.css" rel="stylesheet" />
<link href="@cssPath/jquery.dataTables.min.css" rel="stylesheet" />
<link href="@cssPath/font-awesome.min.css" rel="stylesheet" />
<link href="@cssPath/layout.css" rel="stylesheet" />
<table id="tbl" class="table table-responsive table-striped table-bordered">
    <thead>
        <tr>
            <th data-title="Dia">
                @Html.DisplayNameFor(model => model.Dia)
            </th>
            <th data-title="MesLetra">
                @Html.DisplayNameFor(model => model.Mes)
            </th>
            <th data-title="Descrip">
                @Html.DisplayNameFor(model => model.Descrip)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Dia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MesLetra)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Descrip)
                </td>
            </tr>
        }
    </tbody>
</table>