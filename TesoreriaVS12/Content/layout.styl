#sortable.ui-sortable
	padding 0
	margin 0
	list-style-type none
.ui-sortable-handle
	cursor move
	list-style none
	padding 10px
	border 1px dashed #ccc
	line-height 18px
	margin-bottom 5px
	&:hover
		i
			color lighten(#119548, 5%)
	i
		color lighten(#A6A8A1, 60%)
