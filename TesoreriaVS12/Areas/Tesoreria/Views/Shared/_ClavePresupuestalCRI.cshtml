@model TesoreriaVS12.Areas.Tesoreria.Models.De_ClavePresupuestal

<div id="ParcialPresupuesto">
    <div class="cri">


        <div class="row">
            <div class="col-xs-3 margen-2 labels">
                <label>@Html.LabelFor(model => model.Id_CentroRecaudador)</label>
            </div>
            <div class="col-xs-2 margen-2 inputs">
                <div class="input-group group-egreso-clave">
                    @Html.TextBoxFor(model => model.Id_CentroRecaudador, new { @class = "form-control textBox js_Captura", @objeto = "1" })
