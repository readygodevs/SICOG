@model TesoreriaVS12.Areas.Tesoreria.Models.ModalPresupuestoIngModel
@{
    Layout = null;
}
<div data-title="Buscar Presupuesto de Ingreso" class="TituloModal"></div>
<div data-text="Buscar" class="btnOk"></div>
<div data-text="Cancelar" class="btnCancel"></div>
@using (Html.BeginForm("BuscarPresupuestos", "PresupuestosIngresos", new { area = "Tesoreria" }, FormMethod.Post, new { @id = "frmBuscarPresupuesto", @class = "form-horizontal" }))
{ 
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <input type="hidden" id="TipoBusqueda" value="" />
    <div class="row">
        <div class="col-xs-3 margen-2">
            <label>@Html.LabelFor(model => model.Id_CentroRecaudadorModal)</label>
        </div>
        <div class="col-xs-2 margen-2">
            <div class="input-group">
                @Html.TextBoxFor(model => model.Id_CentroRecaudadorModal, new { @class = "form-control textBoxModal formPresup", @objeto = "1"})
                <span class="input-group-addon js_Buscar">
                    <span class="fa fa-search"></span>
                </span>
            </div>
        </div>
        <div class="col-xs-7 margen-2">
            <div class="col-xs-11">
                <input type="text" id="desc_Id_CentroRecaudadorModal" class="form-control textDescripcion" value="" />
            </div>
            <div class="col-xs-1">
                @Html.ValidationMessageFor(model => model.Id_CentroRecaudadorModal)
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-3 margen-2">
            <label>@Html.LabelFor(model => model.Id_FuenteModal)</label>
        </div>
        <div class="col-xs-2 margen-2">
            <div class="input-group">
                @Html.TextBoxFor(model => model.Id_FuenteModal, new { @class = "form-control textBoxModal formPresup", @objeto = "2"})
                <span class="input-group-addon js_Buscar">
                    <span class="fa fa-search"></span>
                </span>
            </div>
        </div>
        <div class="col-xs-7 margen-2">
            <div class="col-xs-11">
                <input type="text" id="desc_Id_FuenteModal" class="form-control textDescripcion" value="" />
            </div>
            <div class="col-xs-1">
                @Html.ValidationMessageFor(model => model.Id_FuenteModal)
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-3 margen-2">
            <label>@Html.LabelFor(model => model.AnioFinModal)</label>
        </div>
        <div class="col-xs-2 margen-2">
            <div class="input-group">
                @Html.TextBoxFor(model => model.AnioFinModal, new { @class = "form-control textBoxModal formPresup", @objeto = "5" })
                <span class="input-group-addon js_AnioFin">
                    <span class="fa fa-search"></span>
                </span>
            </div>
            @Html.ValidationMessageFor(model => model.AnioFinModal)
        </div>
        <div class="col-xs-7 margen-2">
        </div>
    </div>
    <div class="row">
        <div class="col-xs-3 margen-2">
            <label>@Html.LabelFor(model => model.Id_AlcanceModal)</label>
        </div>
        <div class="col-xs-2 margen-2">
            <div class="input-group">
                @Html.TextBoxFor(model => model.Id_AlcanceModal, new { @class = "form-control textBoxModal formPresup", objeto = "3"})
                <span class="input-group-addon js_Buscar">
                    <span class="fa fa-search"></span>
                </span>
            </div>
        </div>
        <div class="col-xs-7 margen-2">
            <div class="col-xs-11">
                <input type="text" id="desc_Id_AlcanceModal" class="form-control textDescripcion" value="" />
            </div>
            <div class="col-xs-1">
                @Html.ValidationMessageFor(model => model.Id_AlcanceModal)
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-3 margen-2">
            <label>@Html.LabelFor(model => model.Id_ConceptoModal)</label>
        </div>
        <div class="col-xs-2 margen-2">
            <div class="input-group">
                @Html.TextBoxFor(model => model.Id_ConceptoModal, new { @class = "form-control textBoxModal formPresup", objeto = "4", maxlength = 3 })
                <span class="input-group-addon js_Buscar">
                    <span class="fa fa-search"></span>
                </span>
            </div>
        </div>
        <div class="col-xs-7 margen-2">
            <div class="col-xs-11">
                <input type="text" id="desc_Id_ConceptoModal" class="form-control textDescripcion" value="" />
            </div>
            <div class="col-xs-1">
                @Html.ValidationMessageFor(model => model.Id_ConceptoModal)
            </div>
        </div>
    </div>
}
<hr />
<div id="div_resultados"></div>