@model IEnumerable<TesoreriaVS12.Areas.Tesoreria.Models.Ca_Impuestos_Deduccion>
@{
    ViewBag.Title = "Catálogo de Impuestos/Deducciones";
    Layout = null;
    var imagePath = Server.MapPath("~/Images");
    var cssPath = Server.MapPath("~/Content");
}
@Html.Partial("PdfHeader")
<link href="@cssPath/bootstrap.min.css" rel="stylesheet" />
<link href="@cssPath/jquery.dataTables.min.css" rel="stylesheet" />
<link href="@cssPath/font-awesome.min.css" rel="stylesheet" />
<link href="@cssPath/layout.css" rel="stylesheet" />
<table id="tbl" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th data-title="Descripcion">
                Descripción
            </th>
            <th data-title="Id_ImpDed2">
                Clave Impuesto/Deducción
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Descripcion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Id_ImpDed2)
                </td>
            </tr>
        }
    </tbody>
</table>