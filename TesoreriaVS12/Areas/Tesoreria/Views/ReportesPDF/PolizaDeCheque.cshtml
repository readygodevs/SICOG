            width: 110px;
            float:left;
            height:45px;
            line-height:35px;
        }

        .detalle {
            margin-top:45px;
            height:550px;
        }
        .detalle table{
            width:100%;
        }
        .clavep {
            width: 290px;
        }
        .importe {
            width: 237px;
            text-align:right;
        }


        .banco {
            width: 440px;
            float:left;
            height:45px;
            line-height:35px;
            padding-left:10px;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        }
        .cuenta {
            width: 192px;
            float:left;
            height:45px;
            line-height:35px;
            padding-left:10px;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        }
        .nocheque {
            width: 176px;
            float:left;
            height:45px;
            line-height:35px;
            padding-left:10px;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        }
        .importe2 {
            width: 275px;
            float:left;
            text-align:right;
            height:45px;
            line-height:35px;
            padding-left:10px;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
        } 

        .formulo{
            width: 260px;
            float:left;
            height:95px;
            line-height:35px;
            padding-left:10px;
            -moz-box-sizing: border-box;
            box-sizing: border-box;
            overflow:hidden;
        }
        .fila5 {
            margin-top:10px;
            overflow:hidden;
        }

        .nocheque2 {
            text-align:right;
            height:45px;
            line-height:35px;
        }

        .fechalarga {
            height:45px;
            margin-top:60px;
            padding-right:5px;
            text-align:right;
        }

        .fila8 {
            overflow:hidden;
            margin-top:30px;
        }

        .paguese {
            float:left;
            height:45px;
            width:742px;
            line-height:35px;
        }

        .importe3 {
            width:230px;
            height:45px;
            float:right;
            line-height:35px;
        }

        .fila9 {
            margin-top:10px;
        }
      
    </style>
</head>
<body>
    <div id="contenedor">
        <div class="row" id="cabecera"></div>
        <div class="row">
            <div class="beneficiario">NOEL ALEJADRO ESCOBEDO SOLORZANO</div>
        </div>
        <div class="row fila2">
            <div class="contrarecibo">1234567</div>
            <div class="compromiso">1234567</div>
            <div class="fechaemision">02/12/2014</div>
