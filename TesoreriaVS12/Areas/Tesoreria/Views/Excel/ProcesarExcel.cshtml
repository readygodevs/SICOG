@model IEnumerable<TesoreriaVS12.Areas.Tesoreria.Controllers.Valores>

@{
    ViewBag.Title = "ProcesarExcel";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Procesar excel</h2>
<table id="tablaExcel" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Título 1</th>
            <th>Título 2</th>
            <th>Título 3</th>
            <th>Título 4</th>
            <th>Título 5</th>
            <th>Título 6</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Valor1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Valor2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Valor3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Valor4)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Valor5)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Valor6)
                </td>
                <td class="text-center">
                    <i class="fa fa-check js_seleccionarPresupuesto acciones"></i>
                    <i class="fa fa-search js_seleccionarPresupuesto acciones"></i>
                    <i class="fa fa-times js_seleccionarPresupuesto acciones"></i>
                </td>
            </tr>
        }
    </tbody>

</table>
@section Scripts {
    <script type="text/javascript">
        $(document).on("ready", function () {
            ConstruirTabla("tablaExcel");
        });
    </script>
}