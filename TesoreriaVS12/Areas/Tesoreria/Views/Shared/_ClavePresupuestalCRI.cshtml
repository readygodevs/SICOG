                        <span class="fa fa-search"></span>
                    </span>
                </div>
            </div>
            <div class="col-xs-7 margen-2">
                <div class="col-xs-11">
                    @Html.TextBoxFor(model => model.Ca_CentroRecaudador.Descripcion, new { @class = "form-control js_Descripcion egreso-desc" })
                </div>
                <div class="col-xs-1">
                    @Html.ValidationMessageFor(model => model.Id_CentroRecaudador)
                </div>
            </div>
        </div>
