@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PolizaDeCheque</title>
    <style>
        body {
            font-family: "Calibri (Body)";
            font-size:24px;
        }
        #contenedor {
            width: 1100px; 
        }
        .row {
            display: block; 
            clear: both;
        }
        #cabecera {
            height: 160px;
        }
        .beneficiario {
            margin-left:370px;
            font-size:24px;
            line-height:35px;
        }
        .fila2 {
            margin-top:10px;
        }
        .contrarecibo {
            padding-left:90px;
            width: 160px;
            float:left;
            height:45px;
            line-height:35px;
        }
        .compromiso {
            padding-left:110px;
            width: 165px;
            float:left;
            height:45px;
            line-height:35px;
        }
        .fechaemision{
            padding-left:137px;
            width: 165px;
            float:left;
            height:45px;
            line-height:35px;
        }
        .fechadepago {
