@font-face {
  font-family: 'RobotoRegular';
  font-style: normal;
  font-weight: 400;
  src: url(fonts/Roboto/roboto_regular_macroman/Roboto-Regular-webfont.woff);
}
@font-face {
  font-family: 'RobotoBold';
  font-style: normal;
  font-weight: 400;
  src: url(fonts/Roboto/roboto_bold_macroman/Roboto-Bold-webfont.woff);
}

html
	height 100%
body
	height 100%
	h1, h2, h3
		font-family: 'Open Sans', sans-serif;
		font-weight: bold;
	#body
		min-height 100%
	#main
		width 100%
		&.row
			margin-left 0

$height_footer = 100px
.page-wrap 
	min-height 100%
	margin-bottom -($height_footer)
	&:after 
		content ""
		display block
#footer,
.page-wrap:after 
	min-height $height_footer
#footer
	background $rojo_vino


#header
	//position relative
	min-height 100px
	background-color #119548
	height 100px
	color white
	#logo_gob
		float left
		width 238px
	.textos
		float left
		margin 26px 0
		font-size 15px
		color #7e7e7e
		span
			font-family "RobotoRegular"
			&.negritas
				font-family "RobotoBold"
	.titulo
		font-size 20px
		line-height 28px
	.subtitulo
		font-size 15px
		line-height 10px
		text-transform uppercase
	#titulos
		float left
		width 360px
		padding-top 9px

.field-validation-error, .validation-summary-errors
	color red

.sign-out.col-lg-5 
	height 90%
	min-height 90px
	background none repeat scroll 0% 0% #A6A8A1
	text-align right
	img
		display block
		margin 1em auto 6px
	a, 
	span
		color white
	#cerrarSesion
		width 97px
		text-align center
		float left
		a
			text-decoration none
	#ChagePass
		display block
		img
			display inline
			margin 0
	.saludo
		font-weight bold
		color white
		padding-top 10px
	hr 
		margin-top 2px
		margin-bottom 6px


.btn
	border-radius 0px !important
.btn-success
	background #119548 !important
	&:hover
		background darken(#119548, 10%) !important
	&.salir
		padding 10px 30px
		font-size 18px
		
.btn-entrar
	background-color #119548
	transition background-color .5s ease-in
	color white
	border none
	padding 15px 30px
	border-radius 0
	margin-top -7px
	&:hover
		background-color lighten(#119548, 10%)
		color white

.progress
	.progress-bar
		background #119548

.dataTables_wrapper
	#combo_datatable
		select
			height 30px !important
			border 1px solid #DDD
			line-height 30px
	#combo_datatable
		margin-top 20px
	.table
		//margin-top 20px
		th
			border-bottom none
			
.dataTables_wrapper,
.iconos_wraper
	.acciones
		text-align center
		&.izquierda
			text-align left
		font-size 24px
		color #119548
		line-height 20px
		i
			cursor pointer
		a
			color #119548
			cursor pointer
			text-decoration none
		.js_cerrar
			cursor pointer


table#tblSaldos.dataTable tfoot th,
table#tblDetallesTransferencia.dataTable tfoot td
	padding 8px
			
input[type=text],
input[type=password],
input[type=email],
input[type=search],
input[type=number]
